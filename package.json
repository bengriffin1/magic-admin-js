{
  "name": "@magic-sdk/admin",
  "version": "1.9.2",
  "description": "Magic Authentication Admin Javascript SDK.",
  "author": "Magic Labs <team@magic.link>",
  "license": "MIT",
  "repository": "magiclabs/magic-admin-js",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/cjs/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "start": "npm run clean:build && ./scripts/start.sh",
    "build": "npm run clean:build && ./scripts/build.sh",
    "test": "npm run clean:test-artifacts && ./scripts/test.sh",
    "lint": "eslint --fix src/**/*.ts",
    "clean": "npm-run-all -s clean:*",
    "clean:test-artifacts": "rimraf coverage",
    "clean:build": "rimraf dist",
    "clean_node_modules": "rimraf node_modules",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@ikscodes/eslint-config": "^8.4.1",
    "@ikscodes/prettier-config": "^3.0.1",
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.25",
    "@types/node-fetch": "^2.6.11",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "auto": "^11.1.1",
    "boxen-cli": "^4.0.0",
    "eslint": "^8.57.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.0",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.2",
    "npm-run-all": "~4.1.5",
    "prettier": "^3.2.5",
    "rimraf": "~5.0.5",
    "ts-jest": "^29.1.2",
    "ts-node": "~10.9.2",
    "tslint": "~6.1.3"
  },
  "dependencies": {
    "@types/atob": "^2.1.2",
    "atob": "^2.1.2",
    "ethereum-cryptography": "^1.0.1",
    "node-fetch": "^2.6.0",
    "typescript": "^5.4.2",
    "web3": "^4.3.0"
  },
  "lint-staged": {
    "*.{ts,tsx}": "eslint --fix"
  },
  "auto": {
    "plugins": [
      "npm",
      "released"
    ]
  }
}
